#include <stdio.h>

int main() {
  
  int num1;
  int num2;

  int soma, sub, mult, div;

  printf("Primeiro número: ");
  scanf("%d", &num1);
  printf("Segundo número: ");
  scanf("%d", &num2);

  soma = num1 + num2;
  sub = num1 - num2;
  mult = num1 * num2;
  div = num1 / num2;

  printf("Soma: %d,\nSubtração: %d,\nMultiplicação: %d,\nDivisão: %d", soma, sub, mult, div);

}
